program operacoes{
    /* 
    convencao usada para nomear
    a versao do programa de acordo
    com a documentacao <nome prog>_ORIG
    */
    version operacoes_ORIG{ 
        int sucessor(int) = 1;
        int antecessor(int) = 2;
        int quadrado(int) = 3;
        double raiz(int) = 4;
    } = 1;
}=0x20000000; 
