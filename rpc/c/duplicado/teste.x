program operacoes{
    /* 
    convencao usada para nomear a versao 
    do programa de acordo com a documentacao 
    da oracle: <nome prog>_ORIG
    */
    version operacoes_ORIG{ 
        int sucessor(int) = 1;
        int antecessor(int) = 1;
        int quadrado(int) = 2;
    } = 1;
}=0x20000000; 

